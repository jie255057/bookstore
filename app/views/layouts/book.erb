<!DOCTYPE html>
<html>
  <head>
    <title>Bookstore</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'braintree', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%= render 'shared/modal', message: 'aaa' %>
    <%= render 'shared/navbar' %>

  <section class="section">
    <div class="container"> 
      <%= render 'shared/flash' if notice %>
      <section class="columns is-fullheight">
          <%= render 'shared/backend/menu', publishers: Publisher.available ,  categories: Category.all%>
          <div class="column is-10">
            <%= yield %>
          </div>
        </section>
    </div>
  </section>  
  </body>
</html>
